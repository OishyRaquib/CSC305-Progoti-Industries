      <TextArea fx:id="onSaleTextArea" layoutX="14.0" layoutY="148.0" prefHeight="437.0" prefWidth="250.0" />
      <TextField fx:id="carName" layoutX="636.0" layoutY="113.0" prefHeight="35.0" prefWidth="216.0" promptText="Car Name">
         <font>
            <Font name="Monospaced Regular" size="18.0" />
         </font>
      </TextField>
      <TextField fx:id="amountOfCars" layoutX="636.0" layoutY="162.0" prefHeight="35.0" prefWidth="216.0" promptText="Amount">
