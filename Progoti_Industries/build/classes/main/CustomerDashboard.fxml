      </TextField>
      <TextField fx:id="agentPhone" layoutX="636.0" layoutY="314.0" prefHeight="35.0" prefWidth="216.0" promptText="Phone Number">
         <font>
            <Font name="Monospaced Regular" size="18.0" />
         </font>
      </TextField>
      <Button layoutX="403.0" layoutY="525.0" mnemonicParsing="false" onAction="#placeOrderBtn" prefHeight="35.0" prefWidth="216.0" text="Place Order">
         <font>
            <Font name="Monospaced Regular" size="18.0" />
         </font>
